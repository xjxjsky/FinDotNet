{
  "Logging": {
    "LogPath": "/home/LogFiles/myapp.txt",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Serilog": {
      "WriteTo": [
        {
          "Name": "File",
          "Args": {
            "path": "/home/LogFiles/myapp.txt",
            "rollingInterval": "Day"
          }
        }
      ]
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""  
  },
  "JWT": {
    "Issuer": "",
    "Audience": "",
    "SigningKey": ""
  }
}
