{
  "version": "0.2.0",
  "configurations": [
  
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/api/bin/Debug/net8.0/api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/api",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      },
      "launchBrowser": {
        "enabled": true,
        "args": "http://localhost:5177/swagger/index.html",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start http://localhost:5177/swagger/index.html"
        }
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
